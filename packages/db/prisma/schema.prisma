generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
  runtime = "nodejs"
 
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String
  password  String
  createdAt DateTime @default(now())
  role      UserRole @default(MEMBER)

 
}

enum UserRole {
  MEMBER
  ADMIN
}
